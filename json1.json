let products = [
    { name: "Laptop", price: 1000, quantity: 5 },
    { name: "Smartphone", price: 700, quantity: 10 },
    { name: "Headphones", price: 100, quantity: 20 },
    { name: "Monitor", price: 300, quantity: 8 }
];

function calculateTotalRevenue(products) {
    let totalRevenue = products.reduce((sum, product) => {
        return sum + (product.price * product.quantity);
    }, 0);
    return totalRevenue;
}

console.log(calculateTotalRevenue(products)); // الناتج: 22500

